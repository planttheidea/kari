{
  "extends": "rapid7/browser",
  "globals": {
    "__dirname": true,
    "global": true,
    "module": true,
    "process": true,
    "require": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "arrow-body-style": [2, "as-needed"],
    "prefer-arrow-callback": 0,
    "no-confusing-arrow": [2, {"allowParens": true}]
  }
}
